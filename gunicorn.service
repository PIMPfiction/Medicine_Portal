[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/home/ubuntu/medicine_portal/
ExecStart=/usr/bin/gunicorn3 --error-logfile /var/log/gunicorn.log --workers 3 --bind unix:/home/ubuntu/medicine_portal/Medicine_Portal.sock Medicine_Portal.wsgi:application --log-level debug

[Install]
WantedBy=multi-user.target